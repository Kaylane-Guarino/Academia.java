package academia;

public class Academia {

	private double altura;
	private double peso;
	
	public void setaltura(double altura) {
		this.altura = altura;
	}
	
	
	public double getaltura(double altura) {
		return altura;
	}
	
	public void setpeso(double peso) {
		this.peso = peso;
	}

	public double getpeso(double peso) {
		return peso;
	}
}
